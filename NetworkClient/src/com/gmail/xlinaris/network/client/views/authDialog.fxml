<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="202.0" prefWidth="450.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.gmail.xlinaris.network.client.controllers.AuthDialogController">
   <children>
      <Label layoutX="21.0" layoutY="25.0" text="Login" />
      <Label layoutX="14.0" layoutY="72.0" text="Password" />
      <TextField fx:id="loginField" layoutX="87.0" layoutY="21.0" prefHeight="25.0" prefWidth="250.0" promptText="login">
         <padding>
            <Insets left="5.0" />
         </padding></TextField>
      <PasswordField fx:id="passwordField" layoutX="87.0" layoutY="68.0" prefHeight="25.0" prefWidth="250.0" promptText="password">
         <padding>
            <Insets left="5.0" />
         </padding></PasswordField>
      <Button fx:id="authButton" layoutX="87.0" layoutY="108.0" mnemonicParsing="false" onAction="#executeAuth" prefHeight="25.0" prefWidth="149.0" text="Enter Chat" />
      <Label layoutX="16.0" layoutY="149.0" text="Time for authorization:">
         <padding>
            <Insets left="5.0" right="5.0" />
         </padding></Label>
      <Label fx:id="timerOff" alignment="BOTTOM_LEFT" layoutX="175.0" layoutY="148.0" prefWidth="280.0" text="..." textOverrun="WORD_ELLIPSIS" wrapText="true">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
         <opaqueInsets>
            <Insets left="100.0" />
         </opaqueInsets>
      </Label>
   </children>
   <padding>
      <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
   </padding>
</AnchorPane>
